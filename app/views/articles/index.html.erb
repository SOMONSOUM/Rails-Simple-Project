<h1>Listing articles</h1>

<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Text</th>
        <th colspan="2" ></th>
        <!-- <th>Created date</th> -->
    </tr>

    <% @articles.each do |article| %>
        <tr>
            <td>
                <%= article.id %>
            </td>
            <td>
                <%= article.title %>
            </td>
            <td>
                <%= article.text %>
            </td>
            <!-- <td>
                <%= article.created_at %>
            </td> -->
            <td>
                <%= link_to 'Show', article_path(article) %>
            </td>
            <td>
                <%= link_to 'Edit', edit_article_path(article) %>
            </td>
        </tr>
        <% end %>
</table>
<%= link_to  "New Article", new_article_path %>